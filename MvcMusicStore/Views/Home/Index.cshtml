@model List<LinqTo7Dizzle.Entities.Release>
@{
    ViewBag.Title = "ASP.NET MVC Music Store";
}
<div id="promotion">
</div>

<h3><em>Fresh</em> off the grill</h3>

<ul id="album-list">
    @foreach (var release in Model)
    {
        <li><a href="@Url.Action("Details", "Store",
                new { id = release.Id })">

            <img alt="@release.Title" src="@release.ImageUrl" />
            <span>@release.Title</span> </a>
        </li>
    }
</ul>